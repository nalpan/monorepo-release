    "packages/app": {
      "name": "@nalpan/app",
      "version": "1.0.0",
      "dependencies": {
        "@nalpan/module-a": "1.1.0",
        "@nalpan/module-b": "1.0.0"
      }
    },
    "packages/moduleA": {
      "name": "@nalpan/module-a",
      "version": "1.1.0"
    },
    "packages/moduleB": {
      "name": "@nalpan/module-b",
      "version": "1.0.0"
